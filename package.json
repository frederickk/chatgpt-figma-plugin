{
  "name": "chatgpt-figma-plugin",
  "version": "0.3.6",
  "description": "",
  "keywords": [
    "openai",
    "chatgpt",
    "figma",
    "figma-plugin",
    "gpt",
    "machine learning",
    "ai",
    "ml"
  ],
  "main": "build/code.js",
  "scripts": {
    "build": "npm run clean && NODE_ENV=production NODE_NO_WARNINGS=1 NODE_OPTIONS=--loader=ts-node/esm ./node_modules/.bin/webpack",
    "build:dev": "npm run clean && NODE_ENV=development NODE_OPTIONS=--loader=ts-node/esm ./node_modules/.bin/webpack",
    "clean": "rm -rf ./build ./.cache",
    "dev": "NODE_ENV=production NODE_OPTIONS=--loader=ts-node/esm ./node_modules/.bin/webpack --watch",
    "dev:serve": "concurrently \"./node_modules/.bin/webpack serve && npm run dev\" \"npm run serve\"",
    "gen:id": "/usr/local/bin/python3 -c \"exec(\"import random\nrandom.randint(10000000000,99999999999999999999)\")\"",
    "push": "git add --all && git commit --amend --no-edit && git push origin main",
    "serve": "npx tsx ./src/server/server.ts --trace-warnings",
    "test:debug": "curl -X POST http://localhost:3000/debug",
    "test:send": "curl -X POST http://localhost:3000/send -H \"Content-Type: application/json\" -d '{\"text\": \"Explain quantum computing in simple terms\"}'"
  },
  "author": "Ken Frederick <ken.frederick@gmx.de>",
  "license": "None",
  "devDependencies": {
    "@figma/plugin-typings": "^1.56.0",
    "@types/node": "^18.11.16",
    "@types/node-fetch": "^2.6.2",
    "@types/styled-components": "^5.1.11",
    "@types/uuid": "^9.0.0",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-server": "^4.7.2",
    "@types/webpack-env": "^1.18.0",
    "@webpack-cli/serve": "^2.0.0",
    "concurrently": "^7.6.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "eventsource-parser": "^0.0.5",
    "expiry-map": "^2.0.0",
    "figma-plugin-ds": "^1.0.1",
    "html-webpack-inline-source-plugin": "^1.0.0-beta.2",
    "html-webpack-plugin": "^5.5.0",
    "json-formatter-js": "^2.3.4",
    "mini-css-extract-plugin": "^2.7.1",
    "nodemon": "^2.0.20",
    "nunjucks": "^3.2.3",
    "sass": "^1.56.0",
    "sass-loader": "^13.1.0",
    "simple-nunjucks-loader": "^3.2.0",
    "styled-components": "^5.3.0",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.8.4",
    "uuid": "^9.0.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/dotenv-safe": "^8.1.2",
    "@types/express": "^4.17.14",
    "body-parser": "^1.20.1",
    "chatgpt": "^3.3.12",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "dotenv-safe": "^8.2.0",
    "express": "^4.18.2",
    "node-fetch": "^3.3.0",
    "ora": "^6.1.2",
    "tsx": "^3.12.1"
  },
  "engines": {
    "node": ">=17.0"
  },
  "type": "module"
}
